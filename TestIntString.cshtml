@{
    ViewBag.Title = "Test String Connector using Javascript";
   
   @Scripts.Render("~/bundles/jquery","~/bundles/CallConnectorString")
}
<div id="body">
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>Test Data storage for Cloud  Connector using Javascript</h1>

            </hgroup>                
            <div style="float:right;">
                @Html.ActionLink("Test", "Test") | 
                @Html.ActionLink("Test Blob", "TestIntBlob")
            </div>
        </div>
    </section>
    <section class="content-wrapper main-content clear-fix">
